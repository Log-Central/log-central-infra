    {
      "source": "app_logs_exchange",
      "vhost": "/",
      "destination": "logs_pipeline_2",
      "destination_type": "queue",
      "routing_key": "logs_2"
    },
    {
      "source": "app_logs_exchange",
      "vhost": "/",
      "destination": "logs_pipeline_3",
      "destination_type": "queue",
      "routing_key": "logs_3"
    },
    {
      "source": "app_logs_exchange",
      "vhost": "/",
      "destination": "logs_pipeline_4",
      "destination_type": "queue",
      "routing_key": "logs_4"
    },
    {
      "source": "app_logs_exchange",
      "vhost": "/",
      "destination": "worker_tasks_queue",
      "destination_type": "queue",
      "routing_key": "tasks"
    }
  ]
}
