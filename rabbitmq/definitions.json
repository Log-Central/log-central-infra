{
  "vhosts": [{ "name": "/" }],
  "users": [
    {
      "name": "guest",
      "password": "guest",
      "tags": "administrator"
    }
  ],
  "permissions": [
    {
      "user": "guest",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "policies": [],
  "exchanges": [
    {
      "name": "app_logs_exchange",
      "type": "direct",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "queues": [
    {
      "name": "logs_pipeline_1",
      "vhost": "/",
      "durable": true,
      "arguments": { "x-queue-type": "quorum" }
    },
    {
      "name": "logs_pipeline_2",
      "vhost": "/",
      "durable": true,
      "arguments": { "x-queue-type": "quorum" }
    },
    {
      "name": "logs_pipeline_3",
      "vhost": "/",
      "durable": true,
      "arguments": { "x-queue-type": "quorum" }
    },
    {
      "name": "logs_pipeline_4",
      "vhost": "/",
      "durable": true,
      "arguments": { "x-queue-type": "quorum" }
    },
    {
      "name": "worker_tasks_queue",
      "vhost": "/",
      "durable": true,
      "arguments": { "x-queue-type": "quorum" }
    }
  ],
  "bindings": [
    {
      "source": "app_logs_exchange",
      "vhost": "/",
      "destination": "logs_pipeline_1",
      "destination_type": "queue",
